{
    "model": {
        "type": "minecraft:condition",
        "property": "minecraft:has_component",
        "component": "minecraft:custom_name",
        "on_true": {
            "type": "minecraft:select",
            "property": "minecraft:component",
            "component": "minecraft:custom_name",
            "cases": [
                {
                    "when": "PSC - Document",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/psc_document"
                    }
                }
            ], [
                {
                    "when": "DFD - Angry",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/dfd_angry"
                    }
                }
            ], [
                {
                    "when": "DFD - Cry",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/dfd_cry"
                    }
                }
            ], [
                {
                    "when": "DFD - Sweat",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/dfd_sweat"
                    }
                }
            ], [
                {
                    "when": "DFS - Sweat",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/dfs_sweat"
                    }
                }
            ], [
                {
                    "when": "JDG - Gavel",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/jdg_gavel"
                    }
                }
            ], [
                {
                    "when": "CUL - Threaten",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/cul_threaten"
                    }
                }
            ], [
                {
                    "when": "CUL - Sweat",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/cul_sweat"
                    }
                }
            ], [
                {
                    "when": "DFA - Document",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/dfa_document"
                    }
                }
            ], [
                {
                    "when": "DFA - Sweat",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/dfa_sweat"
                    }
                }
            ], [
                {
                    "when": "JRY - Take Notes",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/jry_notes"
                    }
                }
            ], [
                {
                    "when": "JRY - Confident",
                    "model": {
                        "type": "minecraft:model",
                        "model": "minecraft:item/court/jry_confident"
                    }
                }
            ],
            "fallback": {
                "type": "minecraft:model",
                "model": "minecraft:item/slime_ball"
            }
        },
        "on_false": {
            "type": "minecraft:model",
            "model": "minecraft:item/slime_ball"
        }
    }
}
